    <script>
        const thumbnailLink = document.getElementById('thumbnailLink');
        const targetLink = thumbnailLink.href; 

        // ‡¶°‡¶æ‡¶á‡¶®‡¶æ‡¶Æ‡¶ø‡¶ï ‡¶≠‡¶ø‡¶â ‡¶ï‡¶æ‡¶â‡¶®‡ßç‡¶ü‡¶æ‡¶∞ (‡¶Ü‡¶ó‡ßá‡¶∞ ‡¶Æ‡¶§‡ßã)
        const counterElement = document.getElementById('view-counter');
        const localStorageKey = 'cinemaHubViews_2'; 
        const baseViews = 35000; 
        const maxIncrement = 5; 

        let currentViews = parseInt(localStorage.getItem(localStorageKey)) || baseViews;
        
        function formatViews(num) {
            return num.toLocaleString('en-US') + ' Views'; 
        }

        function updateViews() {
            const increment = Math.floor(Math.random() * maxIncrement) + 1; 
            currentViews += increment;
            
            localStorage.setItem(localStorageKey, currentViews);
            
            counterElement.innerHTML = formatViews(currentViews);
        }

        updateViews();

        // ‡¶™‡ßç‡¶∞‡¶§‡¶ø 2 ‡¶∏‡ßá‡¶ï‡ßá‡¶®‡ßç‡¶°‡ßá ‡¶∏‡¶Ç‡¶ñ‡ßç‡¶Ø‡¶æ ‡¶Ü‡¶™‡¶°‡ßá‡¶ü ‡¶ï‡¶∞‡¶æ (‡¶≤‡¶æ‡¶á‡¶≠ ‡¶á‡¶´‡ßá‡¶ï‡ßç‡¶ü)
        setInterval(updateViews, 2000); 
    </script>
