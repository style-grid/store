         */
        function calculateAndSetLayout(gridId) {
            const grid = document.getElementById(gridId);
            if (!grid) return;
            
            const items = Array.from(grid.children);
            if (items.length === 0) return;

            grid.classList.add('temp-layout-mode');
            
            // ‡¶õ‡ßã‡¶ü ‡¶¨‡¶ø‡¶≤‡¶Æ‡ßç‡¶¨ (Delay) ‡¶®‡¶ø‡¶∂‡ßç‡¶ö‡¶ø‡¶§ ‡¶ï‡¶∞‡ßá ‡¶Ø‡ßá ‡¶¨‡ßç‡¶∞‡¶æ‡¶â‡¶ú‡¶æ‡¶∞ ‡¶≤‡ßá‡¶Ü‡¶â‡¶ü ‡¶∞‡ßá‡¶®‡ßç‡¶°‡¶æ‡¶∞ ‡¶ï‡¶∞‡ßá‡¶õ‡ßá 
            setTimeout(() => {
                const positions = items.map((item, index) => {
                    const rect = item.getBoundingClientRect();
                    const gridRect = grid.getBoundingClientRect();
                    return {
                        x: rect.left - gridRect.left,
                        y: rect.top - gridRect.top,
                        width: rect.width,
                        height: rect.height,
                        index: index // ‡¶Ü‡¶á‡¶ü‡ßá‡¶Æ‡ßá‡¶∞ ‡¶á‡¶®‡¶°‡ßá‡¶ï‡ßç‡¶∏ ‡¶¨‡ßç‡¶Ø‡¶¨‡¶π‡¶æ‡¶∞ ‡¶ï‡¶∞‡¶æ
                    };
                });
                initialPositions = positions; // ‡¶™‡ßç‡¶∞‡¶æ‡¶•‡¶Æ‡¶ø‡¶ï ‡¶Ö‡¶¨‡¶∏‡ßç‡¶•‡¶æ‡¶® ‡¶∏‡ßá‡¶≠ ‡¶ï‡¶∞‡¶æ ‡¶π‡¶≤‡ßã

                grid.classList.remove('temp-layout-mode');
                
                const maxBottom = positions.reduce((max, pos) => Math.max(max, pos.y + pos.height), 0);
                grid.style.height = `${maxBottom}px`; 
                grid.style.position = 'relative'; 
                grid.style.visibility = 'visible';

                // ‡¶Ü‡¶á‡¶ü‡ßá‡¶Æ‡¶ó‡ßÅ‡¶≤‡ßã‡¶ï‡ßá absolute ‡¶™‡¶ú‡¶ø‡¶∂‡¶®‡ßá ‡¶§‡¶æ‡¶¶‡ßá‡¶∞ ‡¶™‡ßç‡¶∞‡¶æ‡¶•‡¶Æ‡¶ø‡¶ï ‡¶∏‡ßç‡¶•‡¶æ‡¶®‡ßá ‡¶¨‡¶∏‡¶æ‡¶®‡ßã
                items.forEach((item, index) => {
                    const pos = initialPositions.find(p => p.index === index);
                    
                    if (pos) {
                        item.style.position = 'absolute';
                        item.style.top = `${pos.y}px`;
                        item.style.left = `${pos.x}px`;
                        item.style.width = `${pos.width}px`;
                        item.style.height = `${pos.height}px`;
                        item.style.margin = '0';
                        item.style.transform = 'translate(0, 0)'; // ‡¶™‡ßç‡¶∞‡¶æ‡¶•‡¶Æ‡¶ø‡¶ï ‡¶Ö‡¶¨‡¶∏‡ßç‡¶•‡¶æ‡¶Ø‡¶º ‡¶ï‡ßã‡¶®‡ßã ‡¶ü‡ßç‡¶∞‡¶æ‡¶®‡ßç‡¶∏‡¶´‡¶∞‡ßç‡¶Æ ‡¶•‡¶æ‡¶ï‡¶¨‡ßá ‡¶®‡¶æ
                    }
                });
            }, 50); 
        }

        /**
         * STATIC GRID RENDER FUNCTION
         */
        function renderStaticGrid(gridId, images) {
            const grid = document.getElementById(gridId);
            if (!grid) return;
            
            if (grid.children.length === 0) {
                // ‡¶ó‡ßç‡¶∞‡¶ø‡¶° ‡¶Ü‡¶á‡¶ü‡ßá‡¶Æ‡¶ó‡ßÅ‡¶≤‡¶ø‡¶§‡ßá ‡¶∏‡¶∞‡ßç‡¶¨‡¶¶‡¶æ AD_LINK ‡¶¨‡ßç‡¶Ø‡¶¨‡¶π‡¶æ‡¶∞ ‡¶ï‡¶∞‡¶æ
                grid.innerHTML = images.map((img, index) => `
                    <div class="grid-item" data-index="${index}" onclick="visitLink('${AD_LINK}')">
                        <img src="${img}" alt="Premium Content" loading="lazy">
                        <div class="play-btn-overlay"><i class="fa-solid fa-play"></i></div>
                    </div>
                `).join('');
            }
            
            calculateAndSetLayout(gridId);
        }
        
        /**
         * üî• ‡¶´‡¶ø‡¶ï‡ßç‡¶∏‡¶° SHUFFLE LOGIC
         */
        function shuffleGridItems(gridId) {
            const grid = document.getElementById(gridId);
            if (!grid || initialPositions.length === 0) return;
            
            const items = Array.from(grid.children);
            
            // ‡¶™‡ßç‡¶∞‡¶æ‡¶•‡¶Æ‡¶ø‡¶ï ‡¶Ö‡¶¨‡¶∏‡ßç‡¶•‡¶æ‡¶®‡ßá‡¶∞ ‡¶Ö‡ßç‡¶Ø‡¶æ‡¶∞‡ßá-‡¶è‡¶∞ ‡¶è‡¶ï‡¶ü‡¶ø ‡¶ï‡¶™‡¶ø ‡¶§‡ßà‡¶∞‡¶ø ‡¶è‡¶¨‡¶Ç ‡¶è‡¶≤‡ßã‡¶Æ‡ßá‡¶≤‡ßã ‡¶ï‡¶∞‡¶æ
            const shuffledPositions = [...initialPositions].sort(() => Math.random() - 0.5);

            // ‡¶™‡ßç‡¶∞‡¶§‡¶ø‡¶ü‡¶ø ‡¶Ü‡¶á‡¶ü‡ßá‡¶Æ‡¶ï‡ßá ‡¶è‡¶ï‡¶ü‡¶ø ‡¶è‡¶≤‡ßã‡¶Æ‡ßá‡¶≤‡ßã ‡¶®‡¶§‡ßÅ‡¶® ‡¶™‡¶ú‡¶ø‡¶∂‡¶®‡ßá ‡¶ü‡ßç‡¶∞‡¶æ‡¶®‡ßç‡¶∏‡¶´‡¶∞‡ßç‡¶Æ ‡¶ï‡¶∞‡¶æ 
            items.forEach((item, index) => {
                const initialPos = initialPositions[index]; // ‡¶Ü‡¶á‡¶ü‡ßá‡¶Æ‡ßá‡¶∞ ‡¶¨‡¶∞‡ßç‡¶§‡¶Æ‡¶æ‡¶® ‡¶á‡¶®‡¶°‡ßá‡¶ï‡ßç‡¶∏ ‡¶•‡ßá‡¶ï‡ßá ‡¶§‡¶æ‡¶∞ ‡¶™‡ßç‡¶∞‡¶æ‡¶•‡¶Æ‡¶ø‡¶ï ‡¶Ö‡¶¨‡¶∏‡ßç‡¶•‡¶æ‡¶®
                const newPos = shuffledPositions[index];  // ‡¶∏‡ßá‡¶á ‡¶á‡¶®‡¶°‡ßá‡¶ï‡ßç‡¶∏‡ßá‡¶∞ ‡¶ú‡¶®‡ßç‡¶Ø ‡¶è‡¶≤‡ßã‡¶Æ‡ßá‡¶≤‡ßã‡¶≠‡¶æ‡¶¨‡ßá ‡¶®‡¶ø‡¶∞‡ßç‡¶¨‡¶æ‡¶ö‡¶ø‡¶§ ‡¶®‡¶§‡ßÅ‡¶® ‡¶Ö‡¶¨‡¶∏‡ßç‡¶•‡¶æ‡¶®

                if (initialPos && newPos) {
                    // ‡¶ü‡ßç‡¶∞‡¶æ‡¶®‡ßç‡¶∏‡¶´‡¶∞‡ßç‡¶Æ‡ßá‡¶∞ ‡¶ú‡¶®‡ßç‡¶Ø ‡¶™‡ßç‡¶∞‡¶Ø‡¶º‡ßã‡¶ú‡¶®‡ßÄ‡ßü ‡¶¶‡ßÇ‡¶∞‡¶§‡ßç‡¶¨ ‡¶ó‡¶£‡¶®‡¶æ 
                    const translateX = newPos.x - initialPos.x;
                    const translateY = newPos.y - initialPos.y;
                    
                    // CSS ‡¶ü‡ßç‡¶∞‡¶æ‡¶®‡ßç‡¶∏‡¶´‡¶∞‡ßç‡¶Æ ‡¶™‡ßç‡¶∞‡¶Ø‡¶º‡ßã‡¶ó ‡¶ï‡¶∞‡¶æ
                    item.style.transform = `translate(${translateX}px, ${translateY}px)`;
                }
            });
        }


        // --- TIMER FUNCTIONALITY ---
        let count = 20;
        const timerElement = document.getElementById('timer');
        const waitSection = document.getElementById('wait-section');
        const watchVideoBtn = document.getElementById('watch-video-btn');
        let shuffleInterval; 

        function startTimerAndShuffle() {
            if (!timerElement || !waitSection || !watchVideoBtn || !FINAL_STEP_LINK) {
                return;
            }
            
            // ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶∏‡ßá‡¶ï‡¶∂‡¶® ‡¶¶‡ßÉ‡¶∂‡ßç‡¶Ø‡¶Æ‡¶æ‡¶® ‡¶ï‡¶∞‡¶æ
            waitSection.classList.add('show');
            watchVideoBtn.classList.remove('show-btn'); // ‡¶¨‡¶æ‡¶ü‡¶® ‡¶≤‡ßÅ‡¶ï‡¶ø‡¶Ø‡¶º‡ßá ‡¶∞‡¶æ‡¶ñ‡¶æ

            // üî• ‡¶∂‡¶æ‡¶´‡¶≤‡¶ø‡¶Ç ‡¶∂‡ßÅ‡¶∞‡ßÅ ‡¶ï‡¶∞‡¶æ: ‡ß´ ‡¶∏‡ßá‡¶ï‡ßá‡¶®‡ßç‡¶° (5000ms) ‡¶™‡¶∞ ‡¶™‡¶∞
            shuffleInterval = setInterval(() => {
                shuffleGridItems('static-grid-2');
            }, 5000); // 5 ‡¶∏‡ßá‡¶ï‡ßá‡¶®‡ßç‡¶° ‡¶∏‡ßá‡¶ü ‡¶ï‡¶∞‡¶æ ‡¶π‡¶≤‡ßã

            const countdown = setInterval(() => {
                count--;
                timerElement.textContent = count;

                if (count <= 0) {
                    clearInterval(countdown);
                    clearInterval(shuffleInterval); // ‡¶∂‡¶æ‡¶´‡¶≤‡¶ø‡¶Ç ‡¶¨‡¶®‡ßç‡¶ß ‡¶ï‡¶∞‡¶æ
                    
                    waitSection.innerHTML = '‚¨áÔ∏è Scroll Down and Click Watch Video ‚úÖ'; 
                    // ‡¶ì‡¶Ø‡¶º‡¶æ‡¶ö ‡¶≠‡¶ø‡¶°‡¶ø‡¶ì ‡¶¨‡¶æ‡¶ü‡¶® ‡¶¶‡ßÉ‡¶∂‡ßç‡¶Ø‡¶Æ‡¶æ‡¶® ‡¶ï‡¶∞‡¶æ
                    watchVideoBtn.classList.add('show-btn');
                    // ‡¶∂‡ßÅ‡¶ß‡ßÅ‡¶Æ‡¶æ‡¶§‡ßç‡¶∞ ‡¶è‡¶á ‡¶¨‡¶æ‡¶ü‡¶®‡¶ü‡¶ø FINAL_STEP_LINK ‡¶è ‡¶Ø‡¶æ‡¶¨‡ßá
                    watchVideoBtn.href = FINAL_STEP_LINK; 
                }
            }, 1000);
        }

        
        // Initialize Everything
        
        document.addEventListener('DOMContentLoaded', () => {
             const initialGridImages = imageUrls.slice(0, IMAGES_TO_SHOW_LOGIC); 
            
            // 1. Render Static Grid 2‡•§
            renderStaticGrid('static-grid-2', initialGridImages);

            // 2. Set dynamic click handlers 
            document.getElementById('middle-section').onclick = () => visitLink(AD_LINK);
            document.getElementById('video-thumbnail-container').onclick = () => visitLink(AD_LINK);
            
            // 3. ‡¶¨‡¶æ‡¶ü‡¶®‡¶ó‡ßÅ‡¶≤‡¶ø‡¶∞ href ‡¶∏‡ßá‡¶ü ‡¶ï‡¶∞‡¶æ: 
            document.getElementById('click-package-btn').href = AD_LINK; 
            document.getElementById('get-link-btn').href = AD_LINK; 
            
            // 4. ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶è‡¶¨‡¶Ç ‡¶¨‡¶æ‡¶ü‡¶® ‡¶≤‡ßÅ‡¶ï‡¶æ‡¶®‡ßã ‡¶•‡¶æ‡¶ï‡¶¨‡ßá, ‡¶Ø‡¶¶‡¶ø ‡¶®‡¶æ ‡¶∏‡ßç‡¶ü‡ßá‡¶™ ‡ß© ‡¶ì ‡¶¨‡ßà‡¶ß ‡¶≤‡¶ø‡¶Ç‡¶ï ‡¶•‡¶æ‡¶ï‡ßá
            if (!IS_VALID_STEP) {
                watchVideoBtn.style.display = 'none'; 
                waitSection.style.display = 'none'; 
            }
            
            // 5. ResizeObserver for responsive layout
            const gridElement = document.getElementById('static-grid-2');
            if ('ResizeObserver' in window) {
                const resizeObserver = new ResizeObserver(entries => {
                    let timeout;
                    clearTimeout(timeout);
                    timeout = setTimeout(() => {
                        for (let entry of entries) {
                            if (entry.target.id === 'static-grid-2') {
                                // ‡¶≤‡ßá‡¶Ü‡¶â‡¶ü ‡¶™‡¶∞‡¶ø‡¶¨‡¶∞‡ßç‡¶§‡¶® ‡¶π‡¶≤‡ßá ‡¶™‡¶ú‡¶ø‡¶∂‡¶® ‡¶™‡ßÅ‡¶®‡¶∞‡¶æ‡¶Ø‡¶º ‡¶ó‡¶£‡¶®‡¶æ
                                calculateAndSetLayout('static-grid-2'); 
                                // ‡¶è‡¶∞ ‡¶™‡¶∞‡ßá ‡¶∂‡¶æ‡¶´‡¶≤‡¶ø‡¶Ç ‡¶≤‡¶ú‡¶ø‡¶ï ‡¶ï‡¶æ‡¶ú ‡¶ï‡¶∞‡¶¨‡ßá
                            }
                        }
                    }, 200); 
                });
                resizeObserver.observe(gridElement);
            }
        });
        
        // ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶è‡¶¨‡¶Ç ‡¶∂‡¶æ‡¶´‡¶≤‡¶ø‡¶Ç ‡¶∂‡ßÅ‡¶ß‡ßÅ‡¶Æ‡¶æ‡¶§‡ßç‡¶∞ ‡¶∏‡ßç‡¶ü‡ßá‡¶™ ‡ß© ‡¶è‡¶¨‡¶Ç ‡¶¨‡ßà‡¶ß ‡¶≤‡¶ø‡¶Ç‡¶ï ‡¶•‡¶æ‡¶ï‡¶≤‡ßá ‡¶∂‡ßÅ‡¶∞‡ßÅ ‡¶π‡¶¨‡ßá
        window.addEventListener('load', () => {
            if (IS_VALID_STEP && FINAL_STEP_LINK) {
                 startTimerAndShuffle();
            }
        });
    </script>
</body>
</html>
