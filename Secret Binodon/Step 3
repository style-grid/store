        // --- ‡¶á‡¶â‡¶ü‡¶ø‡¶≤‡¶ø‡¶ü‡¶ø ‡¶´‡¶æ‡¶Ç‡¶∂‡¶® ---
        function decodeUrl(encodedUrl) {
            const base64 = encodedUrl.replace(/-/g, '+').replace(/_/g, '/');
            try {
                 const padding = (4 - base64.length % 4) % 4;
                return atob(base64 + '='.repeat(padding));
            } catch (e) {
                console.error("Decoding error:", e);
                return null;
            }
        }

        // --- CONFIGURATION & URL PARSING ---
        const urlParams = new URLSearchParams(window.location.search);
        const targetParam = urlParams.get('target'); 
        const currentStep = urlParams.get('s');
        
        // üî• ‡ßß. ‡¶Ö‡ßç‡¶Ø‡¶æ‡¶° ‡¶¨‡¶æ ‡¶°‡¶ø‡¶´‡¶≤‡ßç‡¶ü ‡¶≤‡¶ø‡¶Ç‡¶ï (‡¶Ö‡¶®‡ßç‡¶Ø‡¶æ‡¶®‡ßç‡¶Ø ‡¶∏‡¶ï‡¶≤ ‡¶ï‡ßç‡¶≤‡¶ø‡¶ï ‡¶á‡¶≠‡ßá‡¶®‡ßç‡¶ü‡ßá‡¶∞ ‡¶ú‡¶®‡ßç‡¶Ø)
        const AD_LINK = "https://www.effectivegatecpm.com/tqdcx2tmah?key=6eb3606e2d125e4ff02093960ceda032"; 
        
