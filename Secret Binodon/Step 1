        ];
        
        // Initialize Everything
        document.addEventListener('DOMContentLoaded', () => {
             
            // ‡¶ì‡¶Ø‡¶º‡¶æ‡¶®-‡¶ï‡ßç‡¶≤‡¶ø‡¶ï ‡¶á‡¶≠‡ßá‡¶®‡ßç‡¶ü ‡¶π‡ßç‡¶Ø‡¶æ‡¶®‡ßç‡¶°‡¶≤‡¶æ‡¶∞ ‡¶∏‡ßá‡¶ü ‡¶ï‡¶∞‡¶æ
            document.getElementById('middle-section-click').onclick = handleOneClickDefault;
            document.getElementById('video-thumbnail-click').onclick = handleOneClickDefault;
            
