    <script>
        // --- CONFIGURATION ---
        // ‡¶°‡¶ø‡¶´‡¶≤‡ßç‡¶ü ‡¶≤‡¶ø‡¶Ç‡¶ï: ‡¶è‡¶á ‡¶≤‡¶ø‡¶ô‡ßç‡¶ï‡ßá ‡¶ï‡ßç‡¶≤‡¶ø‡¶ï‡¶ó‡ßÅ‡¶≤‡ßã ‡¶Ø‡¶æ‡¶¨‡ßá ‡¶Ø‡¶¶‡¶ø ‡¶§‡¶æ ‡¶Ü‡¶∞‡ßç‡¶ü‡¶ø‡¶ï‡ßá‡¶≤ ‡¶¨‡¶æ‡¶ü‡¶® ‡¶®‡¶æ ‡¶π‡¶Ø‡¶º
        const TARGET_LINK_DEFAULT = "https://prototypesorting.com/p99vwjq2?key=57217116cccb25526dfb6a5a4edd9aea"; 
        
        let NEXT_STEP_FINAL_LINK = TARGET_LINK_DEFAULT; 
        
        let IS_STEP_ACCESS = false; 

        // --- BASE64 DECODING UTILS ---
        function decodeUrl(encodedUrl) {
            const base64 = encodedUrl.replace(/-/g, '+').replace(/_/g, '/');
            try {
                const padding = (4 - base64.length % 4) % 4;
                return atob(base64 + '='.repeat(padding));
            } catch (e) {
                console.error("Decoding error:", e);
                return null;
            }
        }
        
        function visitLink(link) {
            window.location.href = link;
        }

        // üî• ‡¶´‡¶æ‡¶Ç‡¶∂‡¶®: ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶ö‡¶≤‡¶æ‡¶ï‡¶æ‡¶≤‡ßÄ‡¶® ‡¶¨‡¶æ ‡¶°‡ßã‡¶Æ‡ßá‡¶® ‡¶≠‡¶ø‡¶ú‡¶ø‡¶ü‡ßá ‡¶¨‡ßç‡¶Ø‡¶¨‡¶π‡ßÉ‡¶§ ‡¶π‡¶¨‡ßá
        function handleOneClickDefault() {
            visitLink(TARGET_LINK_DEFAULT);
        }

        // --- TIMER & VISIBILITY FUNCTIONALITY ---
        let count = 20;
        const timerElement = document.getElementById('timer');
        const waitSection = document.getElementById('wait-section');
        const nextStepBtn = document.getElementById('next-step-btn'); 
        
        // üí° ‡¶´‡¶∂‡¶®: ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶∂‡ßÅ‡¶∞‡ßÅ ‡¶ï‡¶∞‡¶æ
        function startTimer() {
            if (!timerElement || !waitSection || !nextStepBtn) return; 

            waitSection.classList.add('show-timer');

            const countdown = setInterval(() => {
                count--;
                timerElement.textContent = count;

                if (count <= 0) {
                    clearInterval(countdown);
                    waitSection.innerHTML = '‚¨áÔ∏è Scroll Down and Click the Button ‚úÖ';
                    
                    // üî• ‡¶∂‡ßÅ‡¶ß‡ßÅ‡¶Æ‡¶æ‡¶§‡ßç‡¶∞ ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶∂‡ßá‡¶∑ ‡¶π‡¶≤‡ßá‡¶á ‡¶Ü‡¶∞‡ßç‡¶ü‡¶ø‡¶ï‡ßá‡¶≤‡ßá‡¶∞ ‡¶¨‡¶æ‡¶ü‡¶®‡¶ü‡¶ø ‡¶ö‡ßÇ‡ßú‡¶æ‡¶®‡ßç‡¶§ ‡¶≤‡¶ø‡¶ô‡ßç‡¶ï‡ßá ‡¶∏‡ßá‡¶ü ‡¶è‡¶¨‡¶Ç ‡¶¶‡ßÉ‡¶∂‡ßç‡¶Ø‡¶Æ‡¶æ‡¶® ‡¶ï‡¶∞‡¶æ ‡¶π‡¶≤‡ßã‡•§
                    nextStepBtn.href = NEXT_STEP_FINAL_LINK; 
                    nextStepBtn.classList.add('show-btn');
                    nextStepBtn.style.display = 'block'; 
                    
                    // üî• ‡¶ó‡ßÅ‡¶∞‡ßÅ‡¶§‡ßç‡¶¨‡¶™‡ßÇ‡¶∞‡ßç‡¶£: ‡¶è‡¶ñ‡¶æ‡¶®‡ßá ‡¶Ö‡¶®‡ßç‡¶Ø ‡¶ï‡ßã‡¶®‡ßã ‡¶ï‡ßç‡¶≤‡¶ø‡¶ï‡ßá‡¶¨‡¶≤ ‡¶è‡¶≤‡¶ø‡¶Æ‡ßá‡¶®‡ßç‡¶ü‡ßá‡¶∞ ‡¶≤‡¶ø‡¶Ç‡¶ï ‡¶Ü‡¶™‡¶°‡ßá‡¶ü ‡¶ï‡¶∞‡¶æ ‡¶π‡¶Ø‡¶º‡¶®‡¶ø, ‡¶´‡¶≤‡ßá ‡¶§‡¶æ‡¶∞‡¶æ ‡¶°‡¶ø‡¶´‡¶≤‡ßç‡¶ü ‡¶≤‡¶ø‡¶ô‡ßç‡¶ï‡ßá‡¶á ‡¶•‡¶æ‡¶ï‡¶¨‡ßá‡•§
                }
            }, 1000);
        }

        // üí° ‡¶´‡¶∂‡¶®: URL ‡¶ö‡ßá‡¶ï ‡¶ï‡¶∞‡¶æ ‡¶è‡¶¨‡¶Ç ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶ö‡¶æ‡¶≤‡ßÅ ‡¶ï‡¶∞‡¶æ
        function checkUrlForStepAccess() {
            const urlParams = new URLSearchParams(window.location.search);
            const targetParam = urlParams.get('target'); 
            const isStepOne = urlParams.get('s') === '1';

            if (isStepOne && targetParam) {
                IS_STEP_ACCESS = true; 
                const decodedNextStepLink = decodeUrl(targetParam); 
                
                if (decodedNextStepLink) {
                    NEXT_STEP_FINAL_LINK = decodedNextStepLink;
                    startTimer(); 
                } else {
                     if (waitSection) waitSection.style.display = 'none';
                     if (nextStepBtn) nextStepBtn.style.display = 'none'; 
                }
            } else {
                 if (waitSection) waitSection.style.display = 'none';
                 if (nextStepBtn) nextStepBtn.style.display = 'none'; 
            }
        }

        // --- Marquee Class (‡¶Ö‡¶™‡¶∞‡¶ø‡¶¨‡¶∞‡ßç‡¶§‡¶ø‡¶§, ‡¶∂‡ßÅ‡¶ß‡ßÅ‡¶Æ‡¶æ‡¶§‡ßç‡¶∞ ‡¶∏‡ßç‡¶•‡¶ø‡¶∞ ‡¶ï‡ßç‡¶≤‡¶ø‡¶ï ‡¶π‡ßç‡¶Ø‡¶æ‡¶®‡ßç‡¶°‡¶≤‡¶æ‡¶∞ ‡¶¨‡ßç‡¶Ø‡¶¨‡¶π‡¶æ‡¶∞ ‡¶ï‡¶∞‡¶¨‡ßá) ---
        class InfiniteMarquee {
            
            constructor(containerId, images, speed = 1) {
                this.container = document.getElementById(containerId);
                this.track = this.container.querySelector('.marquee-track');
                this.images = images;
                this.speed = speed; 
                this.pos = 0;
                this.isDragging = false;
                this.startX = 0;
                this.animationID = null;
                this.init();
            }

            init() {
                // Marquee item-‡¶è ‡¶∏‡¶∞‡ßç‡¶¨‡¶¶‡¶æ ‡¶°‡¶ø‡¶´‡¶≤‡ßç‡¶ü ‡¶ï‡ßç‡¶≤‡¶ø‡¶ï ‡¶π‡ßç‡¶Ø‡¶æ‡¶®‡ßç‡¶°‡¶≤‡¶æ‡¶∞ ‡¶¨‡ßç‡¶Ø‡¶¨‡¶π‡¶æ‡¶∞ ‡¶ï‡¶∞‡¶æ ‡¶π‡¶ö‡ßç‡¶õ‡ßá
                const slideHTML = this.images.map(img => `
                    <div class="marquee-item" onclick="handleOneClickDefault()">
                        <img src="${img}" alt="Premium Content" loading="lazy">
                        <div class="play-btn-overlay"><i class="fa-solid fa-play"></i></div>
                    </div>
                `).join('');
                
                this.track.innerHTML = slideHTML + slideHTML + slideHTML;
                // ... (Drag/Touch ‡¶á‡¶≠‡ßá‡¶®‡ßç‡¶ü ‡¶π‡ßç‡¶Ø‡¶æ‡¶®‡ßç‡¶°‡¶≤‡¶æ‡¶∞ ‡¶Ö‡¶™‡¶∞‡¶ø‡¶¨‡¶∞‡ßç‡¶§‡¶ø‡¶§) ...
                this.track.addEventListener('touchstart', (e) => this.touchStart(e), {passive: true});
                this.track.addEventListener('touchmove', (e) => this.touchMove(e), {passive: true});
                this.track.addEventListener('touchend', () => this.touchEnd());
                this.track.addEventListener('mousedown', (e) => this.touchStart(e));
                this.track.addEventListener('mousemove', (e) => this.touchMove(e));
                this.track.addEventListener('mouseup', () => this.touchEnd());
                this.track.addEventListener('mouseleave', () => this.touchEnd());
                this.animate();
            }
            
            // ... (‡¶Ö‡¶®‡ßç‡¶Ø‡¶æ‡¶®‡ßç‡¶Ø Marquee ‡¶Æ‡ßá‡¶•‡¶° ‡¶Ö‡¶™‡¶∞‡¶ø‡¶¨‡¶∞‡ßç‡¶§‡¶ø‡¶§) ...
            getSingleSetWidth() { /* ... */
                const item = this.track.children[0];
                if(!item) return 0;
                const style = window.getComputedStyle(item);
                const gap = parseFloat(style.marginRight || 0) || 15; 
                return (item.offsetWidth + gap) * this.images.length;
            }
            touchStart(e) { /* ... */
                this.isDragging = true;
                this.startX = this.getPositionX(e);
                this.track.style.cursor = 'grabbing';
                cancelAnimationFrame(this.animationID); 
            }
            touchMove(e) { /* ... */
                if (!this.isDragging) return;
                const currentX = this.getPositionX(e);
                const diff = currentX - this.startX;
                this.pos += diff; 
                this.startX = currentX;
                this.updatePosition();
            }
            touchEnd() { /* ... */
                this.isDragging = false;
                this.track.style.cursor = 'grab';
                this.animate(); 
            }
            getPositionX(e) { /* ... */
                return e.type.includes('mouse') ? e.pageX : e.touches[0].clientX;
            }
            updatePosition() { /* ... */
                this.track.style.transform = `translate3d(${this.pos}px, 0, 0)`;
            }
            animate() { /* ... */
                if (!this.isDragging) {
                    this.pos -= this.speed;
                }
                const singleSetWidth = this.getSingleSetWidth(); 
                if (singleSetWidth > 0) {
                    if (this.speed > 0 && this.pos <= -singleSetWidth) {
                        this.pos += singleSetWidth; 
                    }
                    else if (this.speed < 0 && this.pos >= 0) {
                        this.pos -= singleSetWidth; 
                    }
                }
                this.updatePosition();
                this.animationID = requestAnimationFrame(this.animate.bind(this));
            }
        }
        
        const imageUrls = [
            "https://raw.githubusercontent.com/SecretBinodon/vdo/refs/heads/main/Secret%20Binodon/1.jpg", "https://raw.githubusercontent.com/SecretBinodon/vdo/refs/heads/main/Secret%20Binodon/2.jpg",
            "https://raw.githubusercontent.com/SecretBinodon/vdo/refs/heads/main/Secret%20Binodon/3.jpg", "https://raw.githubusercontent.com/SecretBinodon/vdo/refs/heads/main/Secret%20Binodon/4.jpg",
