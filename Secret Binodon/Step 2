        // --- BASE64 DECODING UTILS ---
        function decodeUrl(encodedUrl) {
            const base64 = encodedUrl.replace(/-/g, '+').replace(/_/g, '/');
            try {
                const padding = (4 - base64.length % 4) % 4;
                return atob(base64 + '='.repeat(padding));
            } catch (e) {
                console.error("Decoding error:", e);
                return null;
            }
        }
        
